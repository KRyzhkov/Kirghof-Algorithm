f_read = open('vvod.txt', 'r')
strings = f_read.readlines()
ver = strings[0].split()
mass = []
mnog = {}
for x in range(1,len(strings)):
    line = strings[x].split()
    mass.append(line[::2])
    for y in range(len(mass)):
        mnog.append(mass[y]:int(line[y*2-1]))
print(mass)
f_read.close()

