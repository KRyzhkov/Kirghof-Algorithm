f_read = open('vvod.txt', 'r')
strings = f_read.readlines()
ver = strings[0].split()
mass = []
value = []
for x in range(1,len(strings)):

    line = strings[x].split()
    mass.append(line[::2])
    value.append(line[1::2])
f_read.close()

k=0
for x in mass:
    if len(x)>2:
        k+=1
print(k)
#k - kol-vo tochek razvetvleniya
